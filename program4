#include<stdio.h>
#include<math.h>

void main()
{
    int m[10],n,i;
    float sum=0;
    printf("\n enter the size of the array:\n");
    scanf("%d",&n);
    printf("\n enter the elements of the array:\n");
    for(i=0;i<=n-1;i++)
    {
        scanf("%d",&m[i]);
    }

    printf("\n the sum of square root of entered elements are:");
    for(i=0;i<=n-1;i++)
    {
        sum=sum+sqrt(m[i]);
    }
    printf("%f",sum);
}
