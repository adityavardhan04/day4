5. Smallest of n numbers.


#include<stdio.h>
void main()
{
	int n,a[10],i,smallest;
	printf("\n enter the number:\n");
        scanf("%d",&n);
	
	printf("\n enter the numbers:");
        for(i=0;i<n;i++)
	{
        scanf("%d",&a[i]);
	}
	
	
        for(i=0;i<=n;i++)
	{
    	if(a[i]<smallest)
    	{
            smallest=a[i];
    	}
	}
	printf("\n the smallest is:%d",smallest);
}
